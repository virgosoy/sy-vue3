{
  "name": "sy-vue3",
  "version": "0.1.3-alpha.210909",
  "files": [
    "dist"
  ],
  "main": "./dist/sy-vue3.umd.js",
  "module": "./dist/sy-vue3.es.js",
  "exports": {
    ".": {
      "import": "./dist/sy-vue3.es.js",
      "require": "./dist/sy-vue3.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint lib/**"
  },
  "dependencies": {
    "async-validator": "^3.5.2",
    "bignumber.js": "^9.0.1",
    "vue": "^3.2.10",
    "xlsx": "^0.17.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.6.1",
    "@vitejs/plugin-vue-jsx": "^1.1.8",
    "@vue/compiler-sfc": "^3.2.10",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.17.0",
    "typescript": "^4.3.2",
    "vite": "^2.5.5",
    "vue-tsc": "^0.2.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": ".*",
          "args": "none"
        }
      ]
    }
  }
}
